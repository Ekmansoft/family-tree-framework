{
  "name": "family-tree-framework",
  "version": "1.0.1",
  "description": "A modern React library for visualizing and editing genealogy family trees from GEDCOM files.",
  "main": "./dist/family-tree-framework.umd.js",
  "module": "./dist/family-tree-framework.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/family-tree-framework.es.js",
      "require": "./dist/family-tree-framework.umd.js"
    },
    "./style.css": "./dist/family-tree-framework.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build --config vite.lib.config.ts",
    "build:demo": "vite build --config vite.demo.config.ts",
    "dev": "vite --config vite.demo.config.ts",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "release:patch": "npm version patch && npm run build && git push && git push --tags",
    "release:minor": "npm version minor && npm run build && git push && git push --tags",
    "release:major": "npm version major && npm run build && git push && git push --tags"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ekmansoft/family-tree-framework.git"
  },
  "keywords": [
    "genealogy",
    "family tree",
    "GEDCOM",
    "react",
    "typescript",
    "visualization"
  ],
  "author": "Kenneth Ekman",
  "license": "MIT"
}
